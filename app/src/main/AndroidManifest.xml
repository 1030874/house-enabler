<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.rene.houseenabler" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".View.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".View.CreateUser"
            android:label="@string/title_activity_create_user" >
        </activity>
        <activity
            android:name=".View.Items"
            android:label="@string/title_activity_items" >
        </activity>
        <activity
            android:name=".View.CreateItemParrent"
            android:label="@string/title_activity_create_item_parrent"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity
            android:name=".View.CreateItemChild"
            android:label="@string/title_activity_create_item_child"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
    </application>

</manifest>
